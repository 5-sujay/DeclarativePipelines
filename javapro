pipeline {
    agent {label "Agent Java"}
    stages {
        stage('build1') {
            steps {
                sh '''
                if [ -f .git]
                then
                  ls -l
                cd java-tomcat-maven-example
                mvn install
                else
                  git init
                  git clone https://github.com/deesnest/java-tomcat-maven-example.git
                  cd java-tomcat-maven-example
                  mvn install
                 fi                 
              
                      '''
            }
        }
        
    }
}
