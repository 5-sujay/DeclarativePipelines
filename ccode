  
pipeline {
    agent {label "Agent C"}
    stages {
       stage('gitinit') {
            steps {
                sh '''
                 
                if [ ! -f .git ] && [! -d devtest ]
                then
                git init
                git clone https://github.com/deesnest/devtest
                fi
                  
                 '''
             }
        }
        stage('build1') {
            steps {
            sh '''
                cd devtest
                gcc -c main.c fact.c palin.c rev.c sum.c
                gcc -o ABC.exe main.o fact.o palin.o rev.o sum.o 
                mkdir cbuild
                path=$PWD
                cp $path/ABC.exe $path/cbuild
                echo "C project Build Successful"
                 '''
               
              }
        }
        
    }
}
